	<bean id="equals" class="algoritmi.Equals" scope="singleton">
 		<constructor-arg name="c" ref="editDistance1"></constructor-arg> 
	</bean>
	
	
	
	<bean id="editDistance1" class="algoritmi.EditDistance" scope="singleton">
	<constructor-arg name="c" ref="editDistance2"></constructor-arg> 
	<constructor-arg name="distanza" value="1" type="int"></constructor-arg> 
	</bean>
	
	<bean id="editDistance2" class="algoritmi.EditDistance" scope="singleton">
	<constructor-arg name="c" ref="jaro"></constructor-arg> 
	<constructor-arg name="distanza" value="2" type="int"></constructor-arg> 
	</bean> 
	
	<bean id="jaro" class="algoritmi.Jaro" scope="singleton">
 		<constructor-arg name="c" ref="ratcliff"></constructor-arg> 
	</bean>
		
	<bean id="ratcliff" class="algoritmi.Ratcliff" scope="singleton">
	<constructor-arg name="c" ref="huntMcIlroy"></constructor-arg> 
	</bean>
	
	<bean id="huntMcIlroy" class="algoritmi.HuntMcIlroy" scope="singleton">
<!-- 	<constructor-arg name="next" ref="equals"></constructor-arg>  -->
	</bean>
	
	<!--  <bean id="checkStringDB" class="Algoritmi.CheckStringDB" scope="singleton">
